apiVersion: v1
kind: Secret
metadata:
  name: aerogear-digger
  namespace: {{ namespace }}
  labels:
    jenkins-preexists: "{{ jenkinsPreexists }}"
    serviceInstanceID: '{{ _apb_service_instance_id }}'
stringData:
  type: aerogear-digger
  name: aerogear-digger
  uri: {{ jenkinsUri }}
  displayName: Mobile CI|CD
  statusMessage: {{ statusMessage }}
  statusPercentage: "{{ statusPercentage }}"
